<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white2"
    android:orientation="vertical" >

    <include layout="@layout/title_bar1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/spa72"
        android:background="@drawable/wave_line"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/cb_sequ_by_sum"
                style="@style/normal_textstyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:button="@null"
                android:checked="false"
                android:drawableLeft="@null"
                android:drawablePadding="0px"
                android:drawableRight="@drawable/sequence_backgound_selector"
                android:gravity="center_vertical"
                android:text="@string/sequ1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/cb_sequ_by_pro"
                style="@style/normal_textstyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:button="@null"
                android:checked="false"
                android:drawableLeft="@null"
                android:drawablePadding="0px"
                android:drawableRight="@drawable/sequence_backgound_selector"
                android:gravity="center_vertical"
                android:text="@string/sequ2" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/cb_sequ_by_time"
                style="@style/normal_textstyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:button="@null"
                android:checked="false"
                android:drawableLeft="@null"
                android:drawablePadding="0px"
                android:drawableRight="@drawable/sequence_backgound_selector"
                android:gravity="center_vertical"
                android:text="@string/sequ3" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/cb_sequ_by_interest_rate"
                style="@style/normal_textstyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:button="@null"
                android:checked="false"
                android:drawableLeft="@null"
                android:drawablePadding="0px"
                android:drawableRight="@drawable/sequence_backgound_selector"
                android:gravity="center_vertical"
                android:text="@string/sequ4" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <ListView
            android:id="@+id/lv_projects"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:cacheColorHint="@android:color/transparent"
            android:divider="@drawable/sharp_line"
            android:dividerHeight="1px" >
        </ListView>

        <Button
            android:id="@+id/bt_previous"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/translucent_button_left_selector"
            android:visibility="gone" />

        <Button
            android:id="@+id/bt_next"
            android:layout_width="50dp"
            android:layout_height="100dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/translucent_button_right_selector"
            android:visibility="gone" />
    </RelativeLayout>

</LinearLayout>